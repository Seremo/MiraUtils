                    Source="pack://application:,,,/MahApps.Metro;component/Styles/Themes/Dark.Blue.xaml" />
                <ResourceDictionary Source="/Xceed.Wpf.AvalonDock.Themes.VS2013;component/DarkTheme.xaml" />
                <ResourceDictionary Source="/MiraUI;component/Themes/VS2013/DarkTheme.xaml" />
                <!--<ResourceDictionary Source="/Xceed.Wpf.AvalonDock.Themes.VS2013;component/DarkTheme.xaml" /> / -->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
